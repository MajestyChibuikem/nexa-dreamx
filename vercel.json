{
  "version": 2,
  "builds": [
    {
      "src": "atlasEvolutions/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "atlasEvolutions/wsgi.py"
    }
  ],
  "outputDirectory": ".",
  "installCommand": "pip install -r requirements.txt",
  "functions": {
    "atlasEvolutions/wsgi.py": {
      "includeFiles": "templates/**,static/**,app/**,users/**,_user/**,_admin/**,utils/**,atlasEvolutions/**"
    }
  }
}
